<span style="text-align: right">
<%= form_tag questions_path, :method => 'get' do %>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search" %>
<% end %>
</span>

<h1>List of All Questions</h1>

<% @questions.each do |question| %>
  <h3><p><%= link_to question.title, question %></p></h3>
  <p><%= link_to question.body, question %></p>
  <p>Resolved: <%= question.resolved %></p>
<% end %>

